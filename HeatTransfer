% --- Executes on button press in pushbutton1.
function pushbutton1_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)

% 1-dimensional heat tranfer

clc

global a T0 T1 To m L
% inputs
ro=str2double(get(handles.edit1,'String'));
vod=str2double(get(handles.edit2,'String'));
Cp=str2double(get(handles.edit3,'String'));
T0=str2double(get(handles.edit4,'String'));
T1=str2double(get(handles.edit5,'String'));
To=str2double(get(handles.edit6,'String'));
L=str2double(get(handles.edit7,'String'));
tend=str2double(get(handles.edit8,'String')); %h
krok=str2double(get(handles.edit9,'String')); %h

% ----------------------

ttt=0.001:krok:tend;
a=vod/ro/Cp;
m=0;

disp('  Program starts computing ... ')

for i=1:1:length(ttt)
tkon=ttt(i);
clear x t sol

% solution
x=linspace(0,L,10*L);
t=linspace(0,tkon*3600,10);
sol=pdepe(m,'rovnice','IC','BC',x,t);
u=sol(:,:,1);
uu=sol(end,:,1);

% graphics
AX=sort([T0 To T1]);

plot(handles.axes2,x,uu,'-k','LineWidth',3)
set(handles.axes2,'XMinorTick','on')
grid on
xlabel('x [m]');ylabel('Temperature [C]');
axis([0 L AX(1) AX(3)])

surf(x,t,u)
shading interp
retazec=sprintf('Time=%g h',tkon);
title(retazec)
xlabel('x [m]');ylabel('Time [s]');
axis([0 L 0 3600*tend AX(1) AX(3)])


pause(0.1)
end

disp('End')


